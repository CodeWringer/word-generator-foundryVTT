{{!-- Params:
TEMPLATES: TEMPLATES
presenter: WgFolderContentsPresenter
--}}
{{!-- Folders --}}
{{#with presenter.foldersPresenter.template as |template|}}
  {{> (template) presenter=../presenter.foldersPresenter TEMPLATES=../TEMPLATES }}
{{/with}}
{{!-- Generators --}}
{{#with presenter.generatorsPresenter.template as |template|}}
  {{> (template) presenter=../presenter.generatorsPresenter TEMPLATES=../TEMPLATES }}
{{/with}}
{{!-- Chains --}}
{{#with presenter.chainsPresenter.template as |template|}}
  {{> (template) presenter=../presenter.chainsPresenter TEMPLATES=../TEMPLATES }}
{{/with}}
